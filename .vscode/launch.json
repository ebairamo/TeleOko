{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch TeleOko Server",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/server/main.go",
            "cwd": "${workspaceFolder}",
            "buildFlags": "-tags=debug",
            "env": {
                "GO111MODULE": "on"
            },
            "args": [],
            "showLog": true
        },
        {
            "name": "Launch Current File",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${file}",
            "cwd": "${workspaceFolder}",
            "buildFlags": "-tags=debug",
            "env": {
                "GO111MODULE": "on"
            },
            "args": [],
            "showLog": true
        },
        {
            "name": "Attach to Process",
            "type": "go",
            "request": "attach",
            "mode": "local",
            "processId": 0
        }
    ]
}